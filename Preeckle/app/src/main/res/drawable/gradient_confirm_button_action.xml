<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape xmlns:android="http://schemas.android.com/apk/res/android"
            android:shape="oval">
            <gradient
                android:angle="135"
                android:startColor="@color/colorPrimaryDark"
                android:endColor="@color/colorPrimaryDark"/>
            <size
                android:width="56dp"
                android:height="56dp" />
        </shape>
    </item>
    <item
        android:left="8dp"
        android:right="30dp"
        android:top="30dp"
        android:bottom="20dp">
        <rotate
            android:fromDegrees="-135">
            <shape android:shape="line">
                <stroke
                    android:width="3dp"
                    android:color="#ffffff" />
            </shape>
        </rotate>
    </item>
    <item
        android:left="15dp"
        android:right="7dp">
        <rotate
            android:fromDegrees="135">
            <shape android:shape="line">
                <stroke
                    android:width="3dp"
                    android:color="#ffffff" />
            </shape>
        </rotate>
    </item>
</layer-list>